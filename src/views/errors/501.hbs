<!-- Not Implemented Error Page -->
<div class="error-page">
    <div class="error-container">
        <div class="error-content">
            <div class="error-icon">
                <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="error-svg">
                    <circle cx="12" cy="12" r="10"></circle>
                    <line x1="12" y1="8" x2="12" y2="12"></line>
                    <line x1="12" y1="16" x2="12.01" y2="16"></line>
                </svg>
            </div>
            
            <h1 class="error-title">{{title}}</h1>
            <p class="error-message">{{message}}</p>
            
            <div class="error-actions">
                <a href="/auth/sign-up" class="btn btn-primary">Try Email Signup</a>
                <a href="/" class="btn btn-secondary">Go Home</a>
            </div>
        </div>
    </div>
</div>

