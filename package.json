  "description": "An application to 10X your video content.",
  "main": "src/server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon --ext js,json,hbs,css,hbs,mjs,cjs src/server.js",
    "dev:https": "nodemon --ext js,json,hbs,css,hbs,mjs,cjs src/server-https.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:database": "jest tests/database",
    "test:validation": "jest tests/validation",
    "test:e2e": "jest tests/e2e",
    "test:all": "jest tests/unit tests/database tests/validation tests/integration tests/e2e",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "pretest": "npm run lint",
    "validate": "npm run lint && npm run test:ci",
    "test:comprehensive": "node scripts/test-comprehensive.js"
  },
  "keywords": [
    "nodejs",
    "express",
    "handlebars",
    "postgresql",
    "authentication"
  ],
  "author": "Your Name",
  "license": "MIT",
